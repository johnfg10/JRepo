<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/johnfg10/JUtils</PackageProjectUrl>
    <RepositoryUrl>https://github.com/johnfg10/JUtils</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <NeutralLanguage>English (United Kingdom)</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\JRepo.AspNet\JRepo.AspNet.csproj" />
    <ProjectReference Include="..\JRepo.Core\JRepo.Core.csproj" />
    <ProjectReference Include="..\JRepo.MongoDb\JRepo.MongoDb.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
